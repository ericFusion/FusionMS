}

a.ms-addnew{
color:black !important;
}

.ms-dlgTitleText{
font-weight:bold;
font-family:Arial, Helvetica, sans-serif;
}

.ms-dialog .s4-ca{
background:rgb(235, 229, 229);
}

.ms-menutoolbar td a{
color:#077145;
}

.ms-alternatingstrong {
