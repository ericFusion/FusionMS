}

.ms-dialog body #s4-workspace{
background-color:rgb(235, 229, 229);
background:rgb(235, 229, 229);
}

.ms-dialog .s4-ba{
padding: 15px 10px 0px 10px;
overflow:visible;
}

/*Lists, dialogs*/

.s4-wpActive .s4-wpTopTable, .s4-wpActive .ms-WPSelected{
