/*Macy's intarnet masterpage*/

/*HTML Resets*/

body{
font-family:Arial, Helvetica, sans-serif !important;
}

body.v4master {
height: 100%;
width: 100%;
overflow: hidden !important;
}

/*Background*/

body{
background:white;
overflow:auto;
}

/*Ribbon Hover fixes*/
.ms-siteactionsmenuhover{
border-color: transparent !important;
border-top-color: transparent !important;
background: transparent !important;
background-color: transparent !important;
}

.ms-siteactionsmenuinner{
border-color: transparent !important;
border-top-color: transparent !important;
background: transparent !important;
background-color: transparent !important;
}

.ms-welcomeMenu.ms-SpLinkButtonActive {
border-color: transparent !important;
border-top-color: transparent !important;
background: transparent !important;
background-color: transparent !important;
}

.ms-menu-a:hover span{
text-decoration:underline;
}

.ms-qatbutton{
background-image: none !important;
background-color: transparent !important;
border-color: transparent !important;
}

.s4-breadcrumb-anchor{
background-image: none !important;
background-color: transparent !important;
border-color: transparent !important;
}

/*Top Bar*/
.s4-titletable{
position:relative;
z-index:5;
}

.ms-titlerowborder{
border-right:0px transparent;
}

#s4-titlerow{
width:100% !important;
}

.s4-title{
padding:0px;
background-color:white;
background:white;
min-height:75px;
position:relative;
overflow:hidden;
}
  
.s4-titletext{
display:none;
}

.s4-titlelogo{
float: left;
padding: 0px;
margin-left: 15px;
background-color:#FFFFFF;
border: 0px;
border-bottom: 0px;
}

.s4-title-inner{
float:left;
margin-top:15px;
}


/*Navigation*/

.s4-tn li.static > .menu-item{
height:30px;
}

body #s4-topheader2 {
background-color: rgb(238, 238, 238);
min-height: 40px;
border-top: 0px;
border-bottom:5px solid #077145;
}

.s4-toplinks{
padding:0px;
font-size: 16px;
}

.s4-tn li.static > .menu-item:hover{
background-color:transparent;
border-color:  transparent;
border-bottom-color:  transparent;
border-top-color: transparent;
color:  #077145;
text-decoration:none;
}

.s4-toplinks .s4-tn a.selected{
font-weight:bold;
border-color:  transparent;
border-bottom-color:  transparent;
border-top-color: transparent;
background: transparent;
background-color: transparent;
color: white;
background-color:transparent;
text-decoration:none;
margin:0px;
color: #077145;
}

.s4-toplinks .s4-tn a.selected{
color: #077145 !important;
}

.s4-tn li.static > .menu-item{
text-transform:uppercase;
color:rgb(124, 115, 115);
font-weight:bold;
transition:0.5s color;
-webkit-transition:0.5s background-color;
-moz-transition:0.5s background-color;
-o-transition:0.5s background-color;
}

.s4-tn li.static > .menu-item span{
margin-top:5px;
}

.menu li.hover{
background-color:transparent; /*Needed to keep the parent link highlighted while going through the pages*/
color:black !important;
}

.hover .menu-item-text{
background-color:transparent; !important;
}

.hover ul a:visited{
color:#077145 !important;
transition:0.5s color;
-webkit-transition:0.5s color;
-moz-transition:0.5s color;
-o-transition:0.5s color;
}

.hover ul .menu-item-text{
color:#077145 !important;
transition:0.5s color;
-webkit-transition:0.5s color;
-moz-transition:0.5s color;
-o-transition:0.5s color;
}

.hover ul .menu-item-text:hover{
text-decoration:underline;
background-color:transparent;
}

.hover ul li:hover{
background-color:transparent;
}

.menu-horizontal span.dynamic-children span.additional-background{
padding:0px;
background:transparent;
}

.menu-horizontal a.dynamic-children span.additional-background{
padding:0px;
background:transparent;
}

.s4-tn ul.dynamic {
background: url("/_layouts/images/selbg.png") repeat-x left top;
border:0px;
vertical-align: middle;
background-color:white;
border:5px solid #077145;
border-top:0px;
padding: 10px;
}

.s4-tn li.dynamic > a:hover{
background-color:transparent;
}

/*Search*/
.s4-title .s4-rp{
margin-top:20px;
}

.s4-title .s4-rp{
margin-right:0;
position:relative;
z-index:5;
}

.s4-help{
display:none;
}

.s4-search input.ms-sbplain{
padding:8px;
border:0px !important;
background:transparent;
color: black;
font-style: normal;
border-bottom:2px solid #077145 !important;
}

td.ms-sbgo
{
padding:12px 3px 0px 3px;
}

td.ms-sbgo a{
background-image: url('/Style%20Library/Lloyd/img/searchicon.gif');
background-repeat: no-repeat;
background-position: top left;
display:block;
width:20px;
height:20px;
}

td.ms-sbgo a:hover{
background-image: url('/Style%20Library/Lloyd/img/searchicon.gif');
}

td.ms-sbgo a img{
display: none;
}

/*Body*/
body #s4-mainarea{
margin-top:0px;
}

.s4-ca{
margin-left:308px;
border-top:0px;
border-right:0px;
padding-bottom:20px;
margin-bottom:20px;
border-bottom:solid 5px #077145;
}

/*Left Panel*/
body #s4-leftpanel {
width: 271px;
float: left;
padding-left: 10px;
border: 0px;
border-top: 0px;
border-radius:0px 0px 5px 5px;
-webkit-border-radius:0px 0px 5px 5px;
}

body #s4-leftpanel-content{
border:0px;
background-color:#FFFFFF;
}

.s4-qlheader:visited{
color:#000000;
font-size:1.6em;
transition:0.5s color;
}f

.s4-qlheader{
color:#000000;
font-size:1.6em;
transition:0.5s color;
}

.s4-ql ul.root > li > .menu-item{
color:#000000;
font-size:1.6em;
transition:0.5s color;
text-decoration:none;
font-weight:bold;
}

.s4-ql ul.root > li > .menu-item, .s4-qlheader, .s4-qlheader:visited{
border-style:inherit;
}


.s4-ql li, .s4-specialNavLinkList li{
}

.s4-ql ul.root > li > .menu-item{
border-left: 3px solid rgb(192, 192, 192);
}

.s4-ql ul.root ul{
border-left: 3px solid rgb(192, 192, 192);
}

.s4-specialNavLinkList li{
border-left: 3px solid rgb(192, 192, 192);	
}

.s4-specialNavLinkList li:hover{
border-left: 3px solid #077145;	
}

.s4-ql ul.root ul:hover{
border-left: 3px solid #077145;
}


.s4-ql ul.root > li > .menu-item:hover{
font-weight:bold;
text-decoration:none;
border-left: 3px solid #077145;
}

.s4-specialNavLinkList a:hover > span.ms-splinkbutton-text{
color:#000000 !important;
font-weight:bold;
}


.s4-specialNavLinkList a:hover{
color:#000000 !important;
font-weight:bold;
}

.s4-specialNavLinkList a{
color:#000000 !important;
font-weight:bold;
}

.s4-ql a.selected{
text-decoration:underline;
font-weight:bold;
background:transparent;
background-image:url('');
border:0px !important;
color:#000000 !important;
}
.s4-ql ul.root ul > li > a {
color: rgb(179, 166, 166);
background-color:transparent;
padding-left: 10px;
margin-left: 15px;
border-left: 3px solid rgb(192, 192, 192);

}
.s4-ql ul.root ul > li > a:hover{
text-decoration:underline;
color: black;
font-weight:normal;
border-left-color:#077145;
}

.s4-ql ul.root ul{
padding:0px;
margin:0px;
}

/*Breadcrumbs*/
.breadcrumbs{
background-color:white;
overflow:auto;
}

.breadcrumbs h2{
float: left;
color: black;
margin:10px 20px 10px 20px;
}

.breadcrumbs a:link,.breadcrumbs a:visited{
color:rgb(148, 146, 146);
}

.breadcrumbs .ms-ltviewselectormenuheader .ms-viewselector{
background-color:transparent !important;
}

.breadcrumbs .ms-listdescription{
color: white;
padding-top: 16px;
}

.breadcrumbs .s4-pagedescription{
color:white;
padding-top:5px;
}

/*SharePoint fixes*/

.hide{
display:none;
}

.ms-dialog .s4-ca{
border:0px !important;
}

.ms-dlgTitle{
background-color:#077145;
}

.ms-dialog .ms-bodyareacell{
padding:5px 0px 0px 0px;
}

.s4-ba{
overflow:auto;
}

.ms-dialog .breadcrumbs{
